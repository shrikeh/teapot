{
    "name": "shrikeh/teapot",
    "type": "library",
    "description": "PHP HTTP Response Status library",
    "keywords": ["http"],
    "homepage": "http://shrikeh.github.com/teapot",
    "license": "MIT",
    "authors": [
        {
            "name": "Barney Hanlon",
            "email": "barney@shrikeh.net"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phploc/phploc": ">=1.7.4",
        "pdepend/pdepend": "*",
        "shrink/phpcpd": "dev-master",
        "zerkalica/php-code-browser": "dev-master",
        "phpunit/php-invoker" : ">=1.2.0",
        "phpmd/phpmd" : ">=1.4.0",
        "squizlabs/php_codesniffer": "1.*",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Teapot": "lib" }
    }
}
