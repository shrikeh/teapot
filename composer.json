{
  "name": "shrikeh/teapot",
  "type": "library",
  "description": "PHP HTTP Response Status library",
  "keywords": [
    "http"
  ],
  "homepage": "https://shrikeh.github.io/teapot/",
  "license": "MIT",
  "authors": [
    {
      "name": "Barney Hanlon",
      "email": "barney@shrikeh.net"
    },
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "psr/http-message": "^1.0",
    "teapot/status-code": "^1.0"
  },
  "require-dev": {
    "behat/behat": "^3.2",
    "bossa/phpspec2-expect": "^2.0",
    "escapestudios/symfony2-coding-standard": "^2.9",
    "phpspec/phpspec": "~3.0.0",
    "squizlabs/php_codesniffer": "^2.7"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Teapot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "examples\\Teapot\\": "examples"
    }
  },
  "archive": {
    "exclude": [
      "*",
      "!/src"
    ]
  },
  "prefer-stable": true
}
