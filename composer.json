{
  "name": "shrikeh/teapot",
  "type": "library",
  "description": "PHP HTTP Response Status library",
  "keywords": [
    "http"
  ],
  "homepage": "https://shrikeh.github.io/teapot/",
  "license": "MIT",
  "authors": [
    {
      "name": "Barney Hanlon",
      "email": "barney@shrikeh.net"
    },
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/http-message": "^1.1 || ^2.0",
    "teapot/status-code": "^2.1"
  },
  "require-dev": {
    "ergebnis/php-cs-fixer-config": "^5.9.2",
    "phpspec/phpspec": "^7.4",
    "vimeo/psalm": "^5.14.1"
  },
  "scripts": {
    "analyze": "psalm",
    "cs-check": "php-cs-fixer fix --dry-run",
    "cs-fix": "php-cs-fixer fix",
    "test": "phpspec run"
  },
  "config": {
    "platform": {
      "php": "8.0.29"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Teapot\\": "src"
    }
  },
  "archive": {
    "exclude": [
      "/*, !/src"
    ]
  }
}
